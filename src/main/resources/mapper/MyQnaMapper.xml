<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.start01.mapper.MyQnaMapper">

    <!-- QnA 목록 조회 -->
    <select id="getQnaList" resultType="QnaDto">
        SELECT
        id,
        title,
        q_contents,
        q_answer,
        created_at
        FROM
        qna
        WHERE
        user_id = #{userId}
        ORDER BY
        created_at DESC
    </select>

</mapper>
