<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.start01.mapper.OrdersMapper">

    <select id="findByOrderId" resultType="com.example.start01.dto.OrdersDto">
        SELECT
        order_id,
        room_id,
        user_id,
        store_id,
        room_order,
        total_price,
        created_at
        FROM orders
        WHERE order_id = #{orderId}
    </select>

</mapper>
